<%= simple_form_for(@parking) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :contact %>
    <%= f.input :telephone %>
    <%= f.input :schedule %>
    <%= f.input :price %>
    <%= f.input :availability %>
    <%= f.input :comments %>
    <%= f.input :image %>
    <%= f.input :active %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
