<%= simple_form_for(@rating) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :score %>
    <%= f.input :comment %>
    <%= f.input :active %>
    <%= f.association :parking %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
